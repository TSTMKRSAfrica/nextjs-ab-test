{
  "name": "@tstmkrs/nextjs-ab-test",
  "version": "0.0.2",
  "main": "dist/index.js",
  "dependencies": {
    "react": "^16.13.1"
  },
  "scripts": {
    "lint": "xo --prettier --fix",
    "test": "jest --watch",
    "example": "parcel ./examples/index.html --out-dir output",
    "build": "babel src -d dist --copy-files",
    "pre-push": "npm run lint && npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-jest": "^26.6.2",
    "babel-preset-minify": "^0.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "eslint-plugin-react": "^7.21.5",
    "jest": "^26.6.2",
    "jsdom": "^16.4.0",
    "parcel": "^1.12.4",
    "react-dom": "^16.14.0",
    "xo": "^0.34.1"
  },
  "xo": {
    "ignores": [
      "./tests",
      "./examples",
      "./dist"
    ],
    "extends": [
      "plugin:react/recommended"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      [
        "minify",
        {
          "builtIns": false
        }
      ]
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./tests/_setup-enzyme-adapter.js"
    ]
  }
}
